FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/Alamofire.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/SwiftyJSON.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/UIColor_Hex_Swift.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/UIScrollView_InfiniteScroll.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "Alamofire" -framework "SwiftyJSON" -framework "UIColor_Hex_Swift" -framework "UIScrollView_InfiniteScroll"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-saucer
PODS_ROOT = ${SRCROOT}/Pods